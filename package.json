{
  "name": "Questioner",
  "version": "1.0.0",
  "description": "Questioner is a web app that allows meetups to be created, and allows questions to be posted to the meetups with the options of being voted up or down.",
  "engines":{
    "node": "10.14.2"
  },
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "test": "cross-env NODE_ENV=test nyc mocha Server/test --exit",
    "start": "nodemon --exec babel-node Server/app.js",
    /*"build": "babel Server/app.js --out-file Server/babel/app.js",*/
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.3.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "should": "^13.2.3"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "files": [
      "Server/"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords":[
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
